<?xml version="1.0" encoding="UTF-8"?>

<?context-directive job ctxfile x-setups.ctx ?>

<cd:interface xmlns:cd="http://www.pragma-ade.com/commands">

    <cd:command name="defineparinsert" file="typo-pin.mkxl">
        <cd:arguments>
            <cd:resolve name="keyword-name"/>
            <cd:resolve name="keyword-name-optional"/>
            <cd:assignments list="yes" optional="yes">
                <cd:inherit name="setupparinsert"/>
            </cd:assignments>
        </cd:arguments>
    </cd:command>

    <cd:command name="setupparinsert" file="typo-pin.mkxl">
        <cd:arguments>
            <cd:resolve name="keyword-name-list-optional"/>
            <cd:assignments list="yes">
                <cd:parameter name="xoffset">
                    <cd:constant type="cd:dimension"/>
                </cd:parameter>
                <cd:parameter name="yoffset">
                    <cd:constant type="cd:dimension"/>
                </cd:parameter>
                <cd:parameter name="distance">
                    <cd:constant type="cd:dimension"/>
                </cd:parameter>
                <cd:parameter name="lines">
                    <cd:constant type="cd:number"/>
                </cd:parameter>
                <cd:parameter name="location">
                    <cd:constant type="left" default="yes"/>
                    <cd:constant type="right"/>
                </cd:parameter>
                <cd:parameter name="option">
                    <cd:constant type="depth"/>
                </cd:parameter>
                <cd:parameter name="style">
                    <cd:resolve name="value-style"/>
                </cd:parameter>
                <cd:parameter name="color">
                    <cd:constant type="cd:color"/>
                </cd:parameter>
            </cd:assignments>
        </cd:arguments>
    </cd:command>

    <cd:command name="parinsert" generated="yes" variant="instance" file="typo-pin.mkxl">
        <cd:sequence>
            <cd:instance value="parinsert"/>
        </cd:sequence>
        <cd:arguments>
            <cd:assignments list="yes" optional="yes">
                <cd:inherit name="setupparinsert"/>
            </cd:assignments>
            <cd:content/>
        </cd:arguments>
        <cd:instances>
            <cd:resolve name="instance-parinsert"/>
        </cd:instances>
    </cd:command>

    <cd:command name="pickupparinsert" file="typo-pin.mkxl"/>

    <cd:command name="wrapupparinsert" file="typo-pin.mkxl"/>

</cd:interface>
